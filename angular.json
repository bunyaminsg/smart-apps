{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "qrisk": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/qrisk",
      "sourceRoot": "projects/qrisk/src",
      "prefix": "qrisk",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/qrisk",
            "index": "projects/qrisk/src/index.html",
            "browser": "projects/qrisk/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/qrisk/tsconfig.app.json",
            "assets": [
              "projects/qrisk/src/favicon.ico",
              "projects/qrisk/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/qrisk/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "qrisk:build:production"
            },
            "development": {
              "buildTarget": "qrisk:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "qrisk:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/qrisk/tsconfig.spec.json",
            "assets": [
              "projects/qrisk/src/favicon.ico",
              "projects/qrisk/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/qrisk/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
